{
  "name": "webinsolut-api",
  "version": "1.0.0",
  "scripts": {
    "dev": "node exec.js local.env && tsc -w & nodemon ./bin/www",
    "start": "node exec.js local.env && tsc && node ./bin/www",
    "test": "npm run test-down && docker-compose -f docker/docker-compose.test.yml up --force-recreate --build --abort-on-container-exit",
    "test-down": "docker-compose -f docker/docker-compose.test.yml down && node exec.js test.env",
    "test-local": "./node_modules/.bin/mocha tests/requests/main.js --reporter spec --timeout 10000",
    "test-db-switch": "npm run test-db-local-down && npm run test-db-local",
    "test-db-local": "node exec.js test.local.env && docker-compose -f docker/docker-compose-pgsl.test.yml up -d",
    "test-db-local-down": "docker-compose -f docker/docker-compose-pgsl.test.yml down",
    "db": "node exec.js local.env && docker-compose -f docker/docker-compose-pgsl.yml up -d",
    "db-switch": "npm run db-down && npm run db",
    "db-build": "node exec.js dev.env && docker-compose -f docker/docker-compose-pgsl.yml build --no-cache",
    "db-down": "docker-compose -f docker/docker-compose-pgsl.yml down",
    "production": "docker-compose -f docker/docker-compose.yml up",
    "build": "node exec.js prod.env && docker-compose -f docker/docker-compose.yml build --no-cache",
    "dev-up": "docker-compose -f docker/docker-compose.dev.yml up",
    "dev-build": "node exec.js dev.env && docker-compose -f docker/docker-compose.dev.yml build --no-cache",
    "dev-down": "docker-compose -f docker/docker-compose.dev.yml down",
    "down": "docker-compose -f docker/docker-compose.yml down",
    "logs": "docker-compose -f docker/docker-compose.yml logs",
    "build:prod": "node exec.js prod.env && tsc -p ."
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.3",
    "cloudinary": "^1.19.0",
    "cors": "^2.8.5",
    "datauri": "^2.0.0",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "geoip-lite": "^1.3.8",
    "image-size": "^0.8.3",
    "jsonwebtoken": "^8.5.0",
    "jwt-decode": "^2.2.0",
    "module-alias": "^2.2.2",
    "morgan": "~1.9.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.3.1",
    "nodemon": "^1.17.5",
    "parse-error": "^0.2.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.12.1",
    "pug": "^2.0.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.19.6",
    "sequelize-typescript": "^1.0.0",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.28",
    "@types/cors": "^2.8.6",
    "@types/express-form-data": "^2.0.1",
    "@types/geoip-lite": "^1.1.29",
    "@types/jest": "^24.0.20",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/jwt-decode": "^2.2.1",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.3.10",
    "@types/node": "^12.11.1",
    "@types/nodemailer": "^6.2.2",
    "@types/passport": "^1.0.1",
    "@types/passport-jwt": "^3.0.2",
    "@types/pug": "^2.0.4",
    "@types/validator": "^10.11.3",
    "cross-env": "^6.0.3",
    "jest-date-mock": "^1.0.7",
    "supertest": "^3.4.2",
    "tape": "^4.11.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.4"
  },
  "_moduleAliases": {
    "@app": "lib"
  }
}
